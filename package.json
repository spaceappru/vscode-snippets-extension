{
  "name": "spaceapp-snippets",
  "displayName": "SpaceApp Snippets",
  "description": "Code snippets for SpaceApp",
  "version": "0.0.5",
  "publisher": "spaceapp",
  "icon": "images/logo.jpg",
  "repository": {
    "type": "git",
    "url": "https://github.com/spaceappru/vscode-snippets-extension"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "astro",
        "path": "./snippets/astro.code-snippets"
      },
      {
        "language": "scss",
        "path": "./snippets/scss.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/typescriptreact.code-snippets"
      }
    ]
  }
}
